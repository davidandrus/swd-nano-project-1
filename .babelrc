{
  "presets" : ["es2015", "stage-0", "react"],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "components" : "./components",
      },
      "extensions": [".es6", ".es", ".jsx", ".js", ".es6.js"],
    }]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  }
}
